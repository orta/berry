{
  "name": "@orta/yarn-cli",
  "version": "2.4.0",
  "license": "BSD-2-Clause",
  "main": "./sources/index.ts",
  "dependencies": {
    "@orta/yarn-core": "workspace:^2.4.0",
    "@orta/yarn-fslib": "workspace:^2.4.0",
    "@orta/yarn-libzip": "workspace:^2.2.1",
    "@orta/yarn-parsers": "workspace:^2.3.0",
    "@orta/yarn-plugin-compat": "workspace:^2.2.0",
    "@orta/yarn-plugin-dlx": "workspace:^2.1.4",
    "@orta/yarn-plugin-essentials": "workspace:^2.4.0",
    "@orta/yarn-plugin-file": "workspace:^2.2.0",
    "@orta/yarn-plugin-git": "workspace:^2.3.0",
    "@orta/yarn-plugin-github": "workspace:^2.1.2",
    "@orta/yarn-plugin-http": "workspace:^2.1.2",
    "@orta/yarn-plugin-init": "workspace:^2.2.2",
    "@orta/yarn-plugin-link": "workspace:^2.1.1",
    "@orta/yarn-plugin-node-modules": "workspace:^2.3.0",
    "@orta/yarn-plugin-npm": "workspace:^2.4.0",
    "@orta/yarn-plugin-npm-cli": "workspace:^2.3.0",
    "@orta/yarn-plugin-pack": "workspace:^2.2.3",
    "@orta/yarn-plugin-patch": "workspace:^2.1.2",
    "@orta/yarn-plugin-pnp": "workspace:^2.4.0",
    "@orta/yarn-shell": "workspace:^2.4.1",
    "chalk": "^3.0.0",
    "ci-info": "^2.0.0",
    "clipanion": "^3.0.0-rc.10",
    "fromentries": "^1.2.0",
    "semver": "^7.1.2",
    "tslib": "^1.13.0",
    "typanion": "^3.3.0"
  },
  "devDependencies": {
    "@orta/yarn-builder": "workspace:^2.1.3",
    "@orta/yarn-monorepo": "workspace:0.0.0",
    "@orta/yarn-pnpify": "workspace:^2.4.0",
    "@types/ci-info": "^2",
    "@types/semver": "^7.1.0",
    "micromatch": "^4.0.2",
    "typescript": "4.1.0-beta"
  },
  "peerDependencies": {
    "@orta/yarn-core": "^2.4.0"
  },
  "scripts": {
    "postpack": "rm -rf lib",
    "prepack": "run build:compile \"$(pwd)\"",
    "build:cli+hook": "run build:pnp:hook && yarn-builder build bundle",
    "build:cli": "yarn-builder build bundle",
    "run:cli": "yarn-builder run",
    "update-local": "run build:cli --no-git-hash && rsync -a --delete bundles/ bin/"
  },
  "publishConfig": {
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "bin": null
  },
  "files": [
    "/lib/**/*",
    "!/lib/pluginConfiguration.*",
    "!/lib/cli.*"
  ],
  "@orta/yarn-builder": {
    "bundles": {
      "standard": [
        "@orta/yarn-plugin-essentials",
        "@orta/yarn-plugin-compat",
        "@orta/yarn-plugin-dlx",
        "@orta/yarn-plugin-file",
        "@orta/yarn-plugin-git",
        "@orta/yarn-plugin-github",
        "@orta/yarn-plugin-http",
        "@orta/yarn-plugin-init",
        "@orta/yarn-plugin-link",
        "@orta/yarn-plugin-node-modules",
        "@orta/yarn-plugin-npm",
        "@orta/yarn-plugin-npm-cli",
        "@orta/yarn-plugin-pack",
        "@orta/yarn-plugin-patch",
        "@orta/yarn-plugin-pnp"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/yarnpkg/berry.git",
    "directory": "packages/yarnpkg-cli"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
